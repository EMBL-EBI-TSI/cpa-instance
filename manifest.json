{
  "applicationName": "Generic server instance",
  "contactEmail": "gianni@ebi.ac.uk",
  "about": "A base virtual machine instance",
  "version": "0.5",
  "cloudProviders": [
    {
      "cloudProvider": "AWS",
      "path": "aws",
      "inputs":["machine_type"]
    },
    {
     "cloudProvider": "OSTACK",
     "path": "ostack",
     "inputs":["machine_type"]
    }
  ],
  "inputs": ["disk_image"],
  "deploymentParameters": ["network_name",
                           "floatingip_pool",
                           "vpc_id",
                           "vpc_cidr_block",
                           "subnet_cidr_block"],

  "outputs": ["external_ip"]
}
