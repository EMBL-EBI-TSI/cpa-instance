{
  "applicationName": "Generic server instance",
  "contactEmail": "gianni@ebi.ac.uk",
  "about": "A base virtual machine instance",
  "version": "0.5",
  "cloudProviders": [
    {
      "cloudProvider": "AWS",
      "path": "aws",
      "inputs":["machine_type"]
    },
    {
     "cloudProvider": "OSTACK",
     "path": "ostack",
     "inputs":["machine_type"]
    },
    {
	"cloudProvider": "AZURE",
	"path": "azure",
	"inputs": ["machine_type","region","client_id","client_secret","tenant_id","subscription_id","subnet_id","resource_group","os_image_publisher","os_image_offer","os_image_sku","os_image_version"]
    }
  ],
  "inputs": ["disk_image"],
  "deploymentParameters": ["network_name",
                           "floatingip_pool",
                           "vpc_id",
                           "vpc_cidr_block",
                           "subnet_cidr_block"],

  "outputs": ["external_ip"]
}
